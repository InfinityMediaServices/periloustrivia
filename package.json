{
  "name": "perilous-trivia",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "autoprefixer-stylus": "0.14.0",
    "concurrently": "3.6.1",
    "firebase": "^5.4.1",
    "hashids": "1.1.4",
    "react-router-dom": "^4.3.1",
    "stylus": "0.54.5"
  },
  "dependencies": {
    "history": "4.7.2",
    "prop-types": "^15.6.2",
    "re-base": "4.0.0",
    "react": "16.4.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "16.4.2",
    "react-router": "4.3.1",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "A cool React app that uses firebase to make a multi-player trivia game that might remind you of a TV show.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infn8/periloustrivia.git"
  },
  "keywords": [
    "trivia",
    "game"
  ],
  "author": "Arron Gibson",
  "bugs": {
    "url": "https://github.com/infn8/periloustrivia/issues"
  },
  "homepage": "https://github.com/infn8/periloustrivia#readme"
}
